<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.ssm.weibo.mapper.CollectionsMapper">
	
	<insert id="insertC" parameterType="Collections">
		INSERT INTO
		collections(coid,cuserid,wid) VALUES (coid_seq.nextval,#{cuserid,jdbcType=VARCHAR},#{wid,jdbcType=INTEGER})
	</insert>
	
	<select id="findC" parameterType="string" resultType="list">
		select * from collections where cuserid=#{cuserid}
	</select>

</mapper>