<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.ssm.weibo.mapper.CollectionsMapper">
	<resultMap id="BaseResultMap" type="com.yc.ssm.weibo.entity.Collections">
		<id column="coid" property="coid" jdbcType="INTEGER" />
		<result column="cuserid" property="cuserid" jdbcType="VARCHAR" />
		<result column="wid" property="wid" jdbcType="INTEGER" />
		<result column="cptime" property="cptime" jdbcType="TIMESTAMP" />
	</resultMap>
	
	<insert id="insertC" parameterType="Collections">
		INSERT INTO
		collections(coid,cuserid,wid) VALUES (#{coid},#{cuserid},#{wid})
	</insert>
	
	<select id="findC" parameterType="string" resultType="list">
		select * from collections where cuserid=#{cuserid}
	</select>

</mapper>